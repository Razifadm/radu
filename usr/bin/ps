#!/bin/sh

echo "unlocking steering of GBPS"

wget -O /etc/init.d/qca-nss-ecm \
https://raw.githubusercontent.com/Razifadm/radu/ipk/qca-nss-ecm

uci set network.globals.packet_steering='2'
uci set network.globals.steering_flows='1024'
uci commit network

# ðŸ”š Auto remove installer itself
rm -f "$0"
